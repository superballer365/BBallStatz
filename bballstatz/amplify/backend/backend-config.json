{
	"api": {
		"bballstatzGraphQLApi": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [],
					"defaultAuthentication": {
						"authenticationType": "API_KEY",
						"apiKeyConfig": {
							"description": "api key for bballstatz graphql api",
							"apiKeyExpirationDays": "365"
						}
					}
				}
			}
		}
	},
	"function": {
		"statsFunctionResolver": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda"
		},
		"playerAggregator": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": [
				{
					"category": "api",
					"resourceName": "bballstatzGraphQLApi",
					"attributes": [
						"GraphQLAPIIdOutput",
						"GraphQLAPIEndpointOutput"
					]
				}
			]
		},
		"customAdvancedStatResolver": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda"
		},
		"ballDontLieMigrator": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": [
				{
					"category": "api",
					"resourceName": "bballstatzGraphQLApi",
					"attributes": [
						"GraphQLAPIIdOutput",
						"GraphQLAPIEndpointOutput"
					]
				}
			]
		}
	}
}